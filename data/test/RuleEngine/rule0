internal date = [
   { {</DIGIT>#year "年"}? { </DIGIT>#month "月" </DIGIT>#day "日" } }`dateType="ymd"`
   { ["今日" "今天"]}`dateType="today"`
   { ["明日" "明天"]}`dateType="tomorrow"`
]#date

internal city = { {</place/city>}#city }

internal airport = [
    { @city ["机场" "飞机场"] }
    { @city </place/airport> ["机场" "飞机场"]? }
]#airport 




Place = [
    {@city#placeName}`placeType="city"`
    {@airport#placeName}`placeType="airport"`
]`domain="place"`

Date = [
    { @date }
]`domain="date"`

Flight = [
   { { {"从"}? {[@city @airport]}#from "到" {[@city @airport]}#to} { {"的"}? </domain/ticket/Flight>}? }
   { @date { {"从"}? {[@city @airport]}#from "到" {[@city @airport]}#to} { {"的"}? </domain/ticket/Flight>}? }
   { { {"从"}? {[@city @airport]}#from "到" {[@city @airport]}#to} { @date {"的"}? </domain/ticket/Flight>}? }
]`domain="flight"`

Weather = [
    ^{ { {@city}#where }? ^{ {@date}? {"的"}? { </domain/weather> } }}
]`domain="weather";intent="lookfor"`



choice = [
    {"滨海新区"}
    {"啊"}
]`domain="choice"`
